spring:
  datasource:
    driver-class-name: oracle.jdbc.OracleDriver
    username:
    url: jdbc:oracle:thin:@//your_oracle_host:1521/
    password:
  jpa:
    properties:
      hibernate:
        dialect: org.hibernate.dialect.Oracle12cDialect
        show_sql: 'true'
    hibernate:
      ddl-auto: update
logging:
  level:
    web: debug
